{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "engines": {
        "node": ">=16.0.0"
    },
    "scripts": {
        "build": "vite build",
        "build:node17": "NODE_OPTIONS=--openssl-legacy-provider vite build",
        "dev": "vite",
        "dev:node17": "NODE_OPTIONS=--openssl-legacy-provider vite",
        "dev:full": "concurrently \"npm run dev\" \"php artisan serve --host=0.0.0.0 --port=8000\"",
        "start": "./dev.sh",
        "install:legacy": "npm install --legacy-peer-deps"
    },
    "devDependencies": {
        "@tailwindcss/vite": "^4.0.0",
        "axios": "^1.6.0",
        "concurrently": "^9.0.1",
        "laravel-vite-plugin": "^1.0.5",
        "tailwindcss": "^4.0.0",
        "vite": "^5.4.11",
        "vite-plugin-vue2": "^2.0.3-alpha.1"
    },
    "dependencies": {
        "vue": "^2.7.16",
        "vue-router": "^3.6.5",
        "vue-template-compiler": "^2.7.16",
        "vuex": "^3.6.2"
    }
}
